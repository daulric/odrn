{
  "expo": {
    "name": "odrn",
    "slug": "odrn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/logo.png",
    "scheme": "odrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daulric.odrn",
      "backgroundColor": "#000000", // Add this line
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "We use your microphone for voice calls with friends.",
        "NSCameraUsageDescription": "We use your camera for video calls with friends."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/icon/logo.png",
        "backgroundImage": "./assets/icon/logo.png",
        "monochromeImage": "./assets/icon/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["CAMERA", "RECORD_AUDIO", "MODIFY_AUDIO_SETTINGS", "WAKE_LOCK"],
      "package": "com.daulric.odrn"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon/logo.svg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2b7db9ce-1230-40ca-9b85-3367c714c07c"
      }
    },
    "owner": "daulric",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/2b7db9ce-1230-40ca-9b85-3367c714c07c"
    }
  }
}
